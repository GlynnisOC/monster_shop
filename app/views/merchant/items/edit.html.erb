<h1>Edit Item</h1>

<%= form_tag merchant_items_index_path, method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>

  <%= label_tag :description %>
  <%= text_field_tag :description %>

  <%= label_tag :price %>
  <%= text_field_tag :price %>

  <%= label_tag :inventory %>
  <%= text_field_tag :inventory %>

  <%= submit_tag "Update" %>
<% end %>
